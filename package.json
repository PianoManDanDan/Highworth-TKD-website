{
	"name": "react-site",
	"version": "0.1.0",
	"private": true,
	"homepage": "http://www.highworthtkd.co.uk",
	"dependencies": {
		"@types/node": "^12.12.47",
		"@types/react": "^16.9.36",
		"@types/react-dom": "^16.9.8",
		"@types/react-router-dom": "^5.1.5",
		"babel-jest": "^26.0.1",
		"node-sass": "^4.14.1",
		"react": "^16.13.1",
		"react-app-polyfill": "^1.0.6",
		"react-dom": "^16.13.1",
		"react-icons": "^3.10.0",
		"react-scripts": "3.4.1"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@storybook/addon-a11y": "^5.3.19",
		"@storybook/addon-knobs": "^5.3.19",
		"@storybook/addon-viewport": "^5.3.19",
		"@storybook/react": "^5.3.19",
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.5.0",
		"@testing-library/user-event": "^7.2.1",
		"@types/jest": "^24.9.1",
		"@types/webpack": "^4.41.17",
		"@types/webpack-dev-server": "^3.11.0",
		"@typescript-eslint/eslint-plugin": "^3.2.0",
		"@typescript-eslint/parser": "^3.2.0",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^6.0.2",
		"css-loader": "^3.5.3",
		"eslint": "^7.2.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "^2.21.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "^7.20.0",
		"eslint-plugin-react-hooks": "^4.0.4",
		"gh-pages": "^3.0.0",
		"jest": "^26.0.1",
		"jest-extended": "^0.11.5",
		"jest-junit": "^11.0.0",
		"prettier": "^2.0.5",
		"sass-loader": "^8.0.2",
		"source-map-loader": "^1.0.0",
		"style-loader": "^1.2.1",
		"ts-loader": "^7.0.5",
		"typescript": "^3.7.5",
		"url-loader": "^4.1.0",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^4.2.2"
	},
	"scripts": {
		"start": "webpack-dev-server --open --hot --config configs/webpack.dev.js",
		"build": "webpack --config configs/webpack.prod.js",
		"build:stats": "webpack --config configs/webpack.prod.js --profile --json > webpack-stats.json",
		"view-stats": "npm run build:stats && npx webpack-bundle-analyzer webpack-stats.json dist",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d dist",
		"publish": "npm run predeploy && npm run deploy",
		"test": "jest -i",
		"test:watch": "jest --watch",
		"test:ci": "jest --ci --reporters=default --reporters=jest-junit",
		"storybook": "start-storybook -p 6006 -s ./.storybook/assets",
		"lint": "eslint ./**",
		"lint:fix": "eslint --fix ./**",
		"eslint": "eslint",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest-junit": {
		"outputDirectory": "./test-results",
		"uniqueOutputName": "true",
		"includeConsoleOutput": "true"
	}
}
